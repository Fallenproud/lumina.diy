# Dependency resolution configuration for bolt.diy
legacy-peer-deps=true
fund=false
audit-level=none
prefer-offline=false
registry=https://registry.npmjs.org/

# Resolve specific conflicts
auto-install-peers=true
strict-peer-deps=false

# Build optimization
engine-strict=false
optional=true
save-exact=false

# Security and performance
package-lock=true
shrinkwrap=false